{
    "db_host":"localhost",
    "db_port":"3306",
    "db_name":"icaro",
    "db_user": "{{ icaro.wax.db_username }}",
    "db_password": "{{ icaro.wax.db_password }}",
    "token_expires_days": 1,
    "cors": {
        "origins": ["https://{{ machine_hostname }}"],
        "headers": ["Token", "Content-Type"],
        "methods": ["GET", "PUT", "POST", "DELETE"]
    },
    "auth_social": {
        "facebook": {
            "client_id": "{{ facebook_client_id }}",
            "client_secret": "{{ facebook_client_secret }}",
            "redirect_uri": "http://{{ machine_hostname }}/wings/login/facebook"
        },
        "linkedin": {
            "client_id": "{{ linkedin_client_id }}",
            "client_secret": "{{ linkedin_client_secret }}",
            "redirect_uri": "http://{{ machine_hostname }}/wings/login/linkedin"
        },
        "instagram": {
            "client_id": "{{ instagram_client_id }}",
            "client_secret": "{{ instagram_client_secret }}",
            "redirect_uri": "http://{{ machine_hostname }}/wings/login/instagram"
        }
    },
    "endpoints": {
        "sms": {
            "account_sid": "{{ twilio_account_sid }}",
            "auth_token": "{{ twilio_auth_token }}",
            "number": "{{ twilio_number }}"
        },
        "email": {
            "from": "{{ admin_email }}",
            "smtp_host": "{{ email_smtp_host }}",
            "smtp_port": {{ email_smtp_port }},
            "smtp_user": "{{ email_smtp_user }}",
            "smtp_password": "{{ email_smtp_password }}"
        }
    }
}
